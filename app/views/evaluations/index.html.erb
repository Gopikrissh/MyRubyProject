<p id="notice"><%= notice %></p>

<h1>Listing Evaluations</h1>

<table>
  <thead>
    <tr>
      <th>Coach</th>
      <th>Player tryout</th>
      <th>Rating</th>
      <th>Speed</th>
      <th>Shooting</th>
      <th>Passing</th>
      <th>Comment</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @evaluations.each do |evaluation| %>
      <tr>
        <td><%= evaluation.coach %></td>
        <td><%= evaluation.player_tryout %></td>
        <td><%= evaluation.rating %></td>
        <td><%= evaluation.speed %></td>
        <td><%= evaluation.shooting %></td>
        <td><%= evaluation.passing %></td>
        <td><%= evaluation.comment %></td>
        <td><%= link_to 'Show', evaluation %></td>
        <td><%= link_to 'Edit', edit_evaluation_path(evaluation) %></td>
        <td><%= link_to 'Destroy', evaluation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Evaluation', new_evaluation_path %>
